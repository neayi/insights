{
    "name": "triple-performance/insights",
    "type": "project",
    "description": "Insights",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "albertcht/invisible-recaptcha": "^1.9",
        "doctrine/dbal": "^2.10",
        "spatie/laravel-ignition": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.5",
        "jeroennoten/laravel-adminlte": "3.6.0",
        "laravel/framework": "^9.52.4",
        "laravel/sanctum": "^2.9",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravolt/avatar": "^4.1",
        "nickurt/laravel-stopforumspam": "^1.8",
        "nunomaduro/collision": "^6.1",
        "sentry/sentry-laravel": "^2.9",
        "spatie/laravel-permission": "^5.10.0",
        "spinen/laravel-discourse-sso": "^2.8.0",
        "mailchimp/marketing": "^3.0",
        "sendinblue/api-v3-sdk": "^8.4"
    },
    "require-dev": {
        "knuckleswtf/scribe": "^4.17.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/Seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
